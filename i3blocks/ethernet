#!/bin/bash
INTERFACE=$(ip -br link | grep -E "ens|enp" | head -1 | awk '{print $1}')
if [ -z "$INTERFACE" ]; then
    INTERFACE="eth0"
fi

IP=$(ip -4 addr show "$INTERFACE" 2>/dev/null | grep inet | awk '{print $2}' | cut -d/ -f1)

if [ -n "$IP" ]; then
    echo "E: $IP"
else
    echo "E: down"
fi
